<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.catched_movil">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Catched" >
        <activity android:name=".app.Vista.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".app.Vista.RegistroServicio" />
        <activity android:name=".app.Vista.Login" />
        <activity android:name=".app.Vista.MenuPr" />
        <activity android:name=".app.Vista.RegistroUsuario"/>
        <service android:name="com.catched_movil.app.Vista.Notificacion"/>


    </application>



</manifest>